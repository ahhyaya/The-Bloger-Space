<section>
  <div class="card">
    <a href="/post/{{id}}"></a>
    <div class="card-header">
      {{title}}
    </div>
    <div class="card-body">
      <blockquote class="blockquote mb-0">
        <p>{{content}}</p>
        <footer class="blockquote-footer">Create at
          {{format_date create_at}}
          By User
          {{user.username}}</footer>
      </blockquote>
    </div>
  </div>
</section>